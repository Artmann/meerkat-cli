    "ora": "^8.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "bun-types": "latest",
    "chalk": "^5.4.1",
    "esbuild": "^0.25.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  }
