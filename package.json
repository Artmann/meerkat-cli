{
  "name": "meerkat-cli",
  "version": "1.0.0",
  "description": "A CLI tool that suggests internal links that you can add to your website",
  "author": "Christoffer Artmann <Artgaard@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Artmann/meerkat-cli",
  "keywords": [
    "typescript",
    "seo",
    "internal-links",
    "blazing-fast",
    "ai"
  ],
  "private": false,
